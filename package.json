{
  "name": "slientpdf-print",
  "version": "1.0.0",
  "description": "Node.js module for printing PDF files silently using pdfium",
  "main": "index.js",
  "scripts": {
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "rebuild": "npm run clean && npm run build",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --target 20.0.0 --force --strip --verbose"
  },
  "keywords": [
    "pdf",
    "print",
    "pdfium",
    "node-gyp"
  ],
  "author": "",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "node-gyp": "^10.0.0",
    "prebuildify": "^6.0.1"
  },
  "binary": {
    "napi_versions": [8]
  }
}

